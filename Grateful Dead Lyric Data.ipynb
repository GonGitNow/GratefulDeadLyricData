   "source": [
    "#Step 3 Exploratory Data Analysis\n",
    "\n",
    "df['word_count'] = df['Lyrics'].str.split().str.len()\n",
    "print(f\"Average words per song: {df['word_count'].mean():.2f}\")\n",
    "print(f\"Longest song (words): {df['word_count'].max()}\")\n",
    "print(f\"Shortest song (words): {df['word_count'].min()}\")"
   ]
