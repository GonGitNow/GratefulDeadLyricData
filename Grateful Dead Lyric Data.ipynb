    "#WordCloud of most common words\n",
    "\n",
    "from wordcloud import WordCloud\n",
    "import matplotlib.pyplot as plt\n",
    "all_lyrics = ' '.join(df['Lyrics'].str.lower())\n",
    "wordcloud = WordCloud(width=1600, height=800, background_color='white').generate(all_lyrics)\n",
    "plt.figure(figsize=(20,10))\n",
    "plt.imshow(wordcloud, interpolation='bilinear')\n",
    "plt.axis('off')\n",
    "plt.title('Word Cloud of Grateful Dead Lyrics')\n",
    "plt.savefig('lyrics_wordcloud.png')\n"
   ]
